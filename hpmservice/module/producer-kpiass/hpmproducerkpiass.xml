<configuration>
  <!-- ID of the service. It should be unique accross the Windows system-->
  <id>hpmproducerkpiass</id>
  <!-- Display name of the service -->
  <name>hpmproducerkpiass</name>
  <!-- Service description -->
  <description>hpmproducerkpiass</description>
  <!-- Path to the executable, which should be started -->
  <executable>java</executable>
  <arguments>-Dloader.path="../../lib/" -jar flecy-api-producer-kpiass-3.1.jar</arguments>
  <logpath>logs</logpath>
  <logmode>rotate</logmode>
</configuration>