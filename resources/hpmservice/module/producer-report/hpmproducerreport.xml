<configuration>
  <!-- ID of the service. It should be unique accross the Windows system-->
  <id>hpmproducerreport</id>
  <!-- Display name of the service -->
  <name>hpmproducerreport</name>
  <!-- Service description -->
  <description>hpmproducerreport</description>
  <!-- Path to the executable, which should be started -->
  <executable>java</executable>
  <arguments>-Dloader.path="../../lib/" -jar flecy-api-producer-report-3.1.jar</arguments>
  <logpath>logs</logpath>
  <logmode>rotate</logmode>
</configuration>